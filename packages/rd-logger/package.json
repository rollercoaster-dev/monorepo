{
  "name": "@rollercoaster-dev/rd-logger",
  "version": "0.3.4",
  "type": "module",
  "packageManager": "bun@1.3.2",
  "description": "A neurodivergent-friendly logger for Rollercoaster.dev projects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./core/logger.service": "./dist/core/logger.service.js"
