    "@sinclair/typebox": "^0.34.0",
    "@types/bun": "latest",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.12",
    "@types/on-finished": "^2.3.4",
    "@types/supertest": "6.0.3",
    "elysia": "^1.0.0",
    "eslint": "^9.17.0",
    "express": "^4.19.2",
    "hono": "^4.5.2",
    "on-finished": "^2.4.1",
    "supertest": "7.1.4",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "on-finished": "^2.4.1"
  },
  "peerDependencies": {
    "@sinclair/typebox": ">= 0.34.0",
    "elysia": ">= 1.0.0",
    "express": ">= 4.0.0",
    "hono": ">= 4.0.0"
