{
  "name": "@rollercoaster-dev/shared-config",
  "version": "0.0.0",
  "private": true,
  "description": "Shared configuration for ESLint, TypeScript, and Prettier across the monorepo",
  "type": "module",
  "exports": {
    "./eslint": "./eslint.config.mjs",
    "./prettier": "./prettier.config.mjs",
    "./tsconfig": "./tsconfig.base.json"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write '*.{js,mjs,json}'",
    "format:check": "prettier --check '*.{js,mjs,json}'",
    "test": "echo 'No tests for shared-config'",
    "test:unit": "echo 'No tests for shared-config'",
    "test:integration": "echo 'No tests for shared-config'",
    "test:e2e": "echo 'No tests for shared-config'",
    "type-check": "echo 'No types to check'"
  },
  "files": [
    "eslint.config.mjs",
    "prettier.config.mjs",
    "tsconfig.base.json"
  ],
  "keywords": [
    "eslint",
    "prettier",
    "typescript",
    "config"
  ],
  "dependencies": {
    "@eslint/js": "^9.17.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.6.2",
    "vue-eslint-parser": "^10.2.0"
  },
  "peerDependencies": {
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  }
}
